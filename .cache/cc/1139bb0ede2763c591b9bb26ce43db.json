r-react/i18n';\r\nimport FormControl from '@material-ui/core/FormControl';\r\nimport FormHelperText from '@material-ui/core/FormHelperText';\r\nimport Input from '@material-ui/core/Input';\r\nimport MenuItem from '@material-ui/core/MenuItem';\r\nimport Select from '@material-ui/core/Select';\r\nimport React from 'react';\r\n\r\nexport interface DropdownOption {\r\n    value: string;\r\n    title: string;\r\n    disabled?: boolean;\r\n}\r\n\r\ninterface DropdownProps {\r\n    displayTitle?: string;\r\n    attr: string;\r\n    options: DropdownOption[];\r\n    value?: string;\r\n    disabled?: boolean;\r\n    onChange: (value: string) => void;\r\n    style?: any;\r\n}\r\n\r\nexport default class Dropdown extends React.Component<DropdownProps> {\r\n    render(): React.ReactNode {\r\n        const { displayTitle, attr, options, value, disabled } = this.props;\r\n        return (\r\n            <FormControl style={{ marginTop: -5, ...this.props.style }} disabled={disabled} fullWidth>\r\n                displayTitle \r\n                {/* && <FormHelperText>{I18n.t(displayTitle)}</FormHelperText>} */}\r\n                <Select\r\n                    value={value || '_'}\r\n                    onChange={(e) => this.props.onChange(e.target.value === '_' ? '' : (e.target.value as string))}\r\n                    input={<Input name={attr} id={attr + '-helper'} />}\r\n                    fullWidth\r\n                    style={{ marginTop: -1 }}\r\n                >\r\n                    {options.map((item) => (\r\n                        <MenuItem key={'key-' + item.value} value={item.value || '_'} disabled={item.disabled}>\r\n                            {item.title}\r\n                        </MenuItem>\r\n                    ))}\r\n                </Select>\r\n            </FormControl>\r\n        );\r\n    }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"783047cd572021024f61a392af4d31fe","cacheData":{"env":{}}}